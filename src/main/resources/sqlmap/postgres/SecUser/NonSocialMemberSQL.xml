<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- namespace=com.shoppingmall.admin.repository.MemberMapper -->
<mapper namespace="com.shoppingmall.secuser.NonSocailMember"> 
	<select id="getOneMemberByEmail" >
	  SELECT authid, loginid, email, password, nickname, role
	  FROM nonsocial_member
	  WHERE email =#{email}
	</select>	
	

</mapper>